{
  "version": 2,
  "buildCommand": "bun install",
  "installCommand": "curl -fsSL https://bun.sh/install | bash && ~/.bun/bin/bun install",
  "outputDirectory": ".",
  "framework": null,
  "functions": {
    "src/index.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PORT": "8080"
  }
}
